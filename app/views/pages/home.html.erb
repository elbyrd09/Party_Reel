
<div class="wrapper">

  <%= render 'shared/banner' %>

      <div class="container-carousel">
        <div class="swipe" data-ride="carousel">
          <div id="carousel">
            <div class="hideLeft" id="first">
              <img src= "<%= asset_path("gaby1.jpg")%>" alt="gaby">
            </div>

            <div class="prevLeftSecond" id="second">
              <img src= "<%= asset_path("gaby2.jpg")%>">
            </div>

            <div class="prev" id="third">
              <img src= "<%= asset_path("gaby3.jpg")%>">
            </div>

            <div class="selected" id="fourth">
              <img src= "<%= asset_path("gaby4.jpg")%>">
            </div>

            <div class="next" id="fifth">
              <img src= "<%= asset_path("gaby5.jpg")%>">
            </div>

            <div class="nextRightSecond" id="sixth">
              <img src= "<%= asset_path("gaby7.jpg")%>">
            </div>

            <div class="hideRight" id="seventh">
              <img src= "<%= asset_path("gaby8.jpg")%>">
            </div>
          </div>
        </div>
  <p class="homepage-events">Select or search for an event below</p>
      <%= render 'shared/search_bar' %>
  <div class="card-deck p-5">
      <% @events.each do |event| %>
        <a href="events/<%= event.id %>" class="col-4">
          <div class="card-category" style="background-image: url(<%= asset_path(event.local_img) %>)">
            <div class="event_info">
            </div>
          </div>
        </a>
      <% end %>
    </div>
</div>


